@{
        ViewData["Title"] = "Registrarse - SmarTrip";
    }
<body>
    <form method="post" id="Registrarseform2" asp-controller="Account" onsubmit="return enviarForm();">
        <label id="contactoLabel" for="contacto">Ingrese su email: </label>
        <input type="email" id="contacto" name="email" placeholder="Introducir email" required>
       <a id="toggleLink" onclick="toggleEmailPhone()">¿Usar número de teléfono?</a>
        <label for="contraseña">Ingrese su contraseña: </label>
        <input type="password" id="contraseña" name="contraseña" required>
        <label for="confirmar_contraseña">Confirme su contraseña: </label>
        <input type="password" id="confirmar_contraseña" name="confirmar_contraseña" required>
        <input type="hidden" id="nombre" name="nombre" value = "@ViewBag.Datos.nombre">
        <input type="hidden" id="apellido" name="apellido" value = "@ViewBag.Datos.apellido">
        <input type="hidden" id="fechaNacimiento" name="fechaNacimiento" value = "@ViewBag.Datos.fechaNacimiento">
        <input type="hidden" id ="genero" name="genero" value= "@ViewBag.Datos.genero" >
        <input type="submit" value="Continuar">
    </form>
</body>
